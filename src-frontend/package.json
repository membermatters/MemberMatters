{
  "name": "frontend",
  "version": "3.3.0",
  "description": "The MemberMatters frontend",
  "productName": "MemberMatters",
  "author": "Jaimyn Mayer <github@jaimyn.dev>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "dev:android": "quasar dev -m capacitor -T android",
    "build": "quasar build -m spa",
    "build:pwa": "quasar build -m pwa",
    "build:ios": "node update-ios-version.js && API_BASE_URL=https://portal.brisbanemaker.space quasar build -m capacitor -T ios --ide",
    "build:android": "quasar build -m capacitor -T android --ide",
    "build:electron": "quasar build -m electron",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "npm run format && eslint --cache --fix --ext .js,.vue,.ts ./",
    "lint:precommit": "npm run format && eslint --ext .js,.ts,.vue ./",
    "icons": "icongenie generate -i app-icon.png",
    "icons:bg": "icongenie generate -i app-icon.png -b app-bg.png"
  },
  "dependencies": {
    "@capacitor/core": "^3.5.0",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/storage": "^1.2.5",
    "@ionic/pwa-elements": "^3.0.1",
    "@quasar/cli": "^2.2.1",
    "@quasar/extras": "^1.13.5",
    "@rollup/plugin-inject": "^5.0.3",
    "@sentry/browser": "^6.19.6",
    "@sentry/integrations": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "@sentry/vue": "^6.19.6",
    "@stripe/stripe-js": "^1.29.0",
    "address": "^1.1.2",
    "animate.css": "^4.1.1",
    "axios": "^0.26.1",
    "core-js": "^3.22.2",
    "crypto-js": "^4.1.1",
    "csv-stringify": "^6.4.0",
    "dayjs": "^1.11.1",
    "events": "^3.3.0",
    "html-to-image": "^0.1.3",
    "http-proxy": "^1.18.1",
    "idle-timeout": "^2.0.1",
    "jsrsasign": "^10.5.25",
    "node-stdlib-browser": "^1.2.0",
    "prettier": "^2.6.2",
    "qrcode": "^1.4.4",
    "quasar": "^2.12.2",
    "util": "^0.12.5",
    "vite-tsconfig-paths": "^4.2.0",
    "vue": "^3.2.33",
    "vue-i18n": "^9.2.0-beta.35",
    "vue-router": "^4.0.14",
    "vue2-transitions": "^0.3.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@intlify/vue-i18n-loader": "^5.0.0",
    "@quasar/app-vite": "^1.4.3",
    "@quasar/app-webpack": "3",
    "@quasar/icongenie": "^2.5.1",
    "@types/node": "^17.0.26",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.0.1",
    "devtron": "^1.4.0",
    "electron": "^18.3.7",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^15.5.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-webpack-plugin": "^3.1.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "null-loader": "^4.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite-plugin-rewrite-all": "^1.0.1",
    "xcode": "^3.0.1"
  },
  "engines": {
    "node": "^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
